function setup() {
  createCanvas(400, 400,WEBGL);
}

function draw() {
  background(100, 111, 113);
  
  fill(135,33);
  stroke(7);
  
let x1= sin((frameCount/100)*2*PI);
  let y1= 100*sin((frameCount/200)*2*PI);
  rotateX(frameCount*0.02);
  rotateY(frameCount*0.02);
  rotateZ(frameCount*0.02);
  torus(x1,y1);
  ellipsoid(y1,x1);
  
 

}
